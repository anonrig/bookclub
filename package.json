{
  "name": "bookclub",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "prisma generate --data-proxy && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "graphql-codegen && prisma generate",
    "generate:watch": "graphql-codegen --watch 'src/graphql/**/*.ts'",
    "postinstall": "npm run generate",
    "db:dev": "pscale connect bookclub dev --port 3309",
    "db:prod": "pscale connect bookclub main --port 3309",
    "db:push": "prisma db push"
  },
