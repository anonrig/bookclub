{
  "name": "bookclub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch 'src/graphql/**/*.ts'",
    "postinstall": "npm run generate && prisma generate",
    "db:dev": "pscale connect bookclub dev --port 3309",
    "db:prod": "pscale connect bookclub main --port 3309",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@apollo/link-schema": "2.0.0-beta.3",
    "@graphql-tools/schema": "^8.3.14",
    "@graphql-tools/utils": "^8.6.13",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.15.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "csstype": "^3.1.0",
    "deepmerge": "^4.2.2",
    "graphql": "^16.5.0",
    "graphql-codegen": "^0.4.0",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "postcss": "^8.4.14",
    "prisma": "^3.15.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hot-toast": "^2.2.0",
    "tailwindcss": "^3.1.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lodash.isequal": "^4.5.0",
    "prettier": "^2.6.2"
  }
}
